; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Please don't kill your freinds - Controller"
run/main_scene="res://scenes/lobby/Lobby.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/image="res://images/splashscreen.png"
boot_splash/minimum_display_time=500

[autoload]

LobbyManager="*res://scripts/managers/LobbyManager.gd"
CommunicationManager="*res://scripts/managers/CommunicationManager.gd"
WebSocketMessage="*res://scripts/networking/websocket/WebSocketMessage.gd"
WebSocketConnection="*res://scripts/networking/websocket/WebSocketConnection.gd"
WebRTCConnection="*res://scripts/networking/webrtc/WebRTCConnection.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_debanding=true
